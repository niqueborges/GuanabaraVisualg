Algoritmo "Multiplicação"
//Algoritmo Multiplicação de números positivos
Var numero1, numero2, resultado, contador: Inteiro
Inicio
      Leia(numero1)
      Leia(numero2)
      resultado <- 0
      contador <- 0
      Enquanto contador < numero2 Faça
               resultado <- resultado + numero1
               contador <- contador + 1
      Fim-Enquanto
      Escreva(resultado)

Fimalgoritmo